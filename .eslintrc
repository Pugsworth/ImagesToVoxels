{
    "root": true,
    "parser": "@typescript-eslint/parser",
    "plugins": [
        "@typescript-eslint"
    ],
    "extends": [
        "eslint:recommended",
        "plugin:@typescript-eslint/eslint-recommended",
        "plugin:@typescript-eslint/recommended"
    ],
    "ignorePatterns": [
        ".cache/**",
        "dist/**",
        "node_modules/**"
    ],
    "rules": {
        "@typescript-eslint/no-unused-vars": "warn",
        "no-unused-vars": "warn",
    }
}